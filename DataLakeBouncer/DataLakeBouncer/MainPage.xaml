<Page
    x:Class="DataLakeBouncer.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:muxc="using:Windows.UI.Xaml.Controls"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <ComboBox x:Name="Main_ComboBox_storageList" Margin="91,75,0,0" PlaceholderText="Select DataLake Gen2 namespace" Visibility="Visible" SelectionChanged="Main_ComboBox_storageList_SelectionChanged"></ComboBox>

        <muxc:TreeView x:Name="Main_TreeView_DirList" Margin="91,179,448,74" SelectionMode="Single" ItemInvoked="Main_TreeView_DirList_ItemInvoked" Expanding="Main_TreeView_DirList_Expanding" Collapsed="Main_TreeView_DirList_Collapsing"/>
    </Grid>
</Page>
